name: KK-DNS
desc: 优化 DNS 选择
author: KK
homepage: ""
icon: ""
category: "Optimization"

dns: #!replace
  enable: true
  ipv6: true
  use-system-hosts: true
  prefer-h3: true
  default-nameserver:
    - 10.0.0.11
    - 223.5.5.5
    - 114.114.114.114
    - 119.28.28.28
    - 8.8.8.8
    - 1.1.1.1
    - tls://8.8.4.4:853
    - '[2402:4e00::]:53'
    - '[2400:3200::1]:53'
    - '[2001:4860:4860::8888]:53'
    - '[2606:4700:4700::1111]:53'
  nameserver:
    - 10.0.0.12
    - https://dns.alidns.com/dns-query
    - https://1.12.12.12/dns-query
    - https://doh.pub/dns-query
    - https://doh.360.cn/dns-query
  direct-nameserver:
    - 10.0.0.12
    - https://dns.alidns.com/dns-query
    - https://1.12.12.12/dns-query
    - https://doh.pub/dns-query
    - https://doh.360.cn/dns-query
  fallback:
    - https://doh.pub/dns-query
    - https://1.0.0.1/dns-query
    - https://1.1.1.1/dns-query
    - https://dns.google/dns-query
    - tls://8.8.4.4:853
  fake-ip-filter:
    - geosite:cn
    - private
    - connectivity-check
  nameserver-policy: {}

